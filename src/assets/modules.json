{
  "user": {
    "name": "User",
    "themeCssClass": "user-theme",
    "i18nName": "module.name.users",
    "path": "/user",
    "resourcePath": "/users",
    "requiredPermissions": ["USER_READ", "USER_SEARCH"]
  },
  "category": {
    "name": "Category",
    "themeCssClass": "category-theme",
    "i18nName": "module.name.categories",
    "path": "/category",
    "resourcePath": "/tags",
    "requiredPermissions": ["TAG_READ"],
    "sidenavAddButtonPath": "/category/add",
    "sidenavOptions": [
      {
        "title": "categories.options.search",
        "path": "/category/list",
        "requiredPermissions": ["TAG_READ"]
      },
      {
        "title": "categories.options.add",
        "path": "/category/add",
        "groupId": 1,
        "requiredPermissions": ["TAG_CREATE"]
      },
      {
        "title": "categories.options.edit",
        "path": "/category/0",
        "pathRegex": "/category/\\d+",
        "groupId": 1,
        "requiredPermissions": ["TAG_UPDATE"]
      }
    ]
  },
